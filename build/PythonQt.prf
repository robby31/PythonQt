# profile for non-mevis users to link to PythonQt

include ( python.prf )  

INCLUDEPATH += $$PWD/../src
DEPENDPATH += $$PWD/../src

# check if debug or release
CONFIG(debug, debug|release) {
  DEBUG_EXT = _d 
} else {
  DEBUG_EXT = 
}

win32-msvc* {
  LIBS += /Users/doudou/workspaceQT/MyLibrary/$$QT_VERSION/PythonQt$${DEBUG_EXT}.lib
} else {
  LIBS += -L/Users/doudou/workspaceQT/MyLibrary/$$QT_VERSION -lPythonQt$${DEBUG_EXT}
}
