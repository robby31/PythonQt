# profile for non-mevis users to link to PythonQt

include ( python.prf )  

INCLUDEPATH += $$PWD/../src
DEPENDPATH += $$PWD/../src

# check if debug or release
CONFIG(debug, debug|release) {
 mac {
   DEBUG_EXT = _debug
 } else {
   DEBUG_EXT = _d
 }
} else {
  DEBUG_EXT = 
}

!exists($$(MYLIBRARY)) {
    error("variable MYLIBRARY not set.")
}

win32-msvc* {
  LIBS += $$(MYLIBRARY)/$$QT_VERSION/PythonQt$${DEBUG_EXT}.lib
} else {
  LIBS += -L$$(MYLIBRARY)/$$QT_VERSION -lPythonQt$${DEBUG_EXT}
}
